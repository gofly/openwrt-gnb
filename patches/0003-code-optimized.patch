+        if (log != NULL && log->output_type != GNB_LOG_OUTPUT_NONE &&                            \
+            (log->config_table[log_id].console_level >= level ||                                 \
+             log->config_table[log_id].file_level    >= level ||                                 \
+             log->config_table[log_id].udp_level     >= level)) {                                \
